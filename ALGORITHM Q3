ENTER NUM1 AND NUM2
ELSE IF OPERATION ="+"
THEN RESULT= NUM1+NUM2
ELSE IF OPERATION ="-"
THEN RESULT= NUM1-NUM2
ELSE IF OPERATION ="*"
THEN RESULT= NUM1*NUM2
ELSE IF OPERRATION= "/" AND NUM2=0
THEN PRINT"ERROR.PLEASE ENTER A VALID NUMBER"
ELSE RESULT=NUM1/NUM2
IF OPERATION ="%"
AND NUM2=0 THEN 
DISPLAY"ERROR,0 IS NOT ALLOWED" 
ELSE RESULT= NUM1/NUM2*100
IF OPERATION IS NOT" +OR-OR*OR/OR%"THEN 
PRINT"ENTER A VALID OPERATION"
DISPLAY RESULT
